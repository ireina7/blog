\module {

  
  (\set (\color \c \self)) {
    (\font (\set \style color:\c) \self)
  }
  (\set \Skeleton) {
    (\color [purple] Skeleton)
  }
    
  (\set (\ref \h \self)) {
    (\link (\set \href \h) \self)
  }

  (\set (\head \self)) {
    (\## (\bold \self))\line
  }

  \doc { to generate indented paragraph }
  (\set (\para \self)) {
    \paragraph {
      (\set \style text-indent:2em)
      \self
    }
  }
}